local U=game:GetService("UserInputService")local C=game:GetService("CoreGui")local C3=Color3.fromRGB local function D(f)local u=U local d,g,m,p false,nil,nil,nil f.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 then d=true m=i.Position g=f.Position i.Changed:Connect(function()if i.UserInputState==Enum.UserInputState.End then d=false end end)end end) f.InputChanged:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseMovement then p=i end end) u.InputChanged:Connect(function(i)if d and i==p then local x,y=i.Position.X-m.X,i.Position.Y-m.Y f.Position=UDim2.new(g.X.Scale,g.X.Offset+x,g.Y.Scale,g.Y.Offset+y)end end)end local UI={}UI.__index=UI function UI:cw(t,s,k)local o=setmetatable({fs={},cur=nil},UI)local sc=Instance.new("ScreenGui",C)sc.Name="UIlib"local f=Instance.new("Frame",sc)f.Size=s f.Position=UDim2.new(0.5,-s.X.Offset/2,0.5,-s.Y.Offset/2)f.BackgroundColor3=C3(6,63,32)f.BorderSizePixel=0 f.Active=true Instance.new("UICorner",f) D(f) local tb=Instance.new("Frame",f)tb.Size=UDim2.new(1,0,0,30)tb.BackgroundColor3=C3(10,80,42)tb.BorderSizePixel=0 Instance.new("UICorner",tb) local tl=Instance.new("TextLabel",tb)tl.Size=UDim2.new(1,-30,1,0)tl.Position=UDim2.new(0,10,0,0)tl.BackgroundTransparency=1 tl.Text=t tl.TextColor3=C3(210,255,210) tl.TextScaled=true tl.TextXAlignment=Enum.TextXAlignment.Left local cb=Instance.new("TextButton",tb)cb.Size=UDim2.new(0,30,1,0)cb.Position=UDim2.new(1,-30,0,0)cb.Text="âœ•" cb.BackgroundColor3=C3(40,100,50) cb.TextColor3=C3(210,255,210) cb.TextScaled=true cb.BorderSizePixel=0 Instance.new("UICorner",cb) cb.MouseButton1Click:Connect(function()f.Visible=false end) local tbf=Instance.new("Frame",f) tbf.Size=UDim2.new(1,0,0,30) tbf.Position=UDim2.new(0,0,0,30) tbf.BackgroundColor3=C3(10,80,42) tbf.BorderSizePixel=0 Instance.new("UICorner",tbf) o.tbf=tbf local cf=Instance.new("Frame",f) cf.Size=UDim2.new(1,0,1,-60) cf.Position=UDim2.new(0,0,0,60) cf.BackgroundTransparency=1 o.cf=cf U.InputBegan:Connect(function(i,gp)if not gp and i.KeyCode==k then f.Visible=not f.Visible end end) return o end function UI:NewTab(n)local f=Instance.new("ScrollingFrame",self.cf) f.Size=UDim2.new(1,0,1,0) f.BackgroundTransparency=1 f.CanvasSize=UDim2.new(0,0,0,0) f.ScrollBarThickness=5 f.Visible=false f.AutomaticCanvasSize=Enum.AutomaticSize.Y f.VerticalScrollBarInset=Enum.ScrollBarInset.ScrollBar local l=Instance.new("UIListLayout",f) l.SortOrder=Enum.SortOrder.LayoutOrder l.Padding=UDim.new(0,8) local o={f=f,btns={},dds={},sls={},cps={},tbs={}} if not self.cur then f.Visible=true self.cur=o end local fs=self.fs fs[#fs+1]=o local b=Instance.new("TextButton",self.tbf) local len=#fs b.Size=UDim2.new(0,100,1,0) b.Position=UDim2.new(0,(len-1)*100,0,0) b.Text=n b.BackgroundColor3=C3(10,80,42) b.BorderSizePixel=0 b.TextColor3=C3(210,255,210) b.TextScaled=true Instance.new("UICorner",b) b.MouseButton1Click:Connect(function() for i=1,#fs do fs[i].f.Visible=false end f.Visible=true self.cur=o end) function o:Button(t,cb)local b=Instance.new("TextButton",f) b.Size=UDim2.new(1,-20,0,30) b.BackgroundColor3=C3(15,100,50) b.BorderSizePixel=0 b.Text=t b.TextColor3=C3(210,255,210) b.TextScaled=true b.LayoutOrder=#self.btns+1 b.MouseButton1Click:Connect(function()if cb then cb()end end) self.btns[#self.btns+1]=b return b end function o:Slider(t,mn,mx,d,cb)local f=Instance.new("Frame",self.f) f.Size=UDim2.new(1,-20,0,40) f.BackgroundColor3=C3(15,100,50) f.BorderSizePixel=0 f.LayoutOrder=#self.btns+#self.dds+1 local lbl=Instance.new("TextLabel",f) lbl.Size=UDim2.new(0.3,-10,1,0) lbl.Position=UDim2.new(0,10,0,0) lbl.BackgroundTransparency=1 lbl.TextColor3=C3(210,255,210) lbl.Text=t lbl.TextScaled=true lbl.TextXAlignment=Enum.TextXAlignment.Left local bg=Instance.new("Frame",f) bg.Size=UDim2.new(0.45,-10,0,20) bg.Position=UDim2.new(0.3,10,0.5,-10) bg.BackgroundColor3=C3(25,130,70) bg.BorderSizePixel=0 local fill=Instance.new("Frame",bg) fill.Size=UDim2.new((d-mn)/(mx-mn),0,1,0) fill.BackgroundColor3=C3(60,230,120) fill.BorderSizePixel=0 local tb=Instance.new("TextBox",f) tb.Size=UDim2.new(0.2,-10,0,25) tb.Position=UDim2.new(0.78,0,0.5,-12) tb.BackgroundColor3=C3(25,130,70) tb.TextColor3=C3(210,255,210) tb.Text=tostring(d) tb.ClearTextOnFocus=false tb.TextScaled=true tb.BorderSizePixel=0 Instance.new("UICorner",tb) local dwn=false bg.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 then dwn=true end end) bg.InputEnded:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 then dwn=false end end) local function u(x)local r=math.clamp(x-bg.AbsolutePosition.X,0,bg.AbsoluteSize.X) fill.Size=UDim2.new(r/bg.AbsoluteSize.X,0,1,0)local v=mn+((r/bg.AbsoluteSize.X)*(mx-mn)) v=math.clamp(v,mn,mx) tb.Text=string.format("%.2f",v) lbl.Text=t..": "..string.format("%.2f",v) if cb then cb(v) end return v end U.InputChanged:Connect(function(i)if dwn and i.UserInputType==Enum.UserInputType.MouseMovement then u(U:GetMouseLocation().X)end end) tb.FocusLost:Connect(function(e)if e then local n=tonumber(tb.Text)if n then n=math.clamp(n,mn,mx) fill.Size=UDim2.new((n-mn)/(mx-mn),0,1,0) lbl.Text=t..": "..string.format("%.2f",n) if cb then cb(n) end else tb.Text=string.format("%.2f",mn)end end end) lbl.Text=t..": "..string.format("%.2f",d)return f end function o:Dropdown(t,o,cb)local df=Instance.new("Frame",self.f) df.Size=UDim2.new(1,-20,0,40) df.BackgroundColor3=C3(15,100,50) df.BorderSizePixel=0 df.LayoutOrder=#self.btns+1 local lbl=Instance.new("TextLabel",df) lbl.Size=UDim2.new(1,-20,1,0) lbl.Position=UDim2.new(0,10,0,0) lbl.BackgroundTransparency=1 lbl.TextColor3=C3(210,255,210) lbl.Text=t..":" lbl.TextScaled=true lbl.TextXAlignment=Enum.TextXAlignment.Left local sel=Instance.new("TextButton",df) sel.Size=UDim2.new(0,120,0,30) sel.Position=UDim2.new(1,-130,0,5) sel.BackgroundColor3=C3(20,120,60) sel.TextColor3=C3(210,255,210) sel.Text=o[1] sel.TextScaled=true sel.BorderSizePixel=0 Instance.new("UICorner",sel) local of=Instance.new("Frame",df) of.Size=UDim2.new(0,120,0,#o*30) of.Position=UDim2.new(1,-130,0,35) of.BackgroundColor3=C3(15,95,50) of.Visible=false of.BorderSizePixel=0 of.ClipsDescendants=true local l=Instance.new("UIListLayout",of) l.SortOrder=Enum.SortOrder.LayoutOrder for i=1,#o do local b=Instance.new("TextButton",of) b.Size=UDim2.new(1,0,0,30) b.BackgroundColor3=C3(20,110,55) b.BorderSizePixel=0 b.Text=o[i] b.TextColor3=C3(210,255,210) b.TextScaled=true b.LayoutOrder=i b.MouseButton1Click:Connect(function() sel.Text=o[i] of.Visible=false if cb then cb(o[i]) end end) end sel.MouseButton1Click:Connect(function() of.Visible=not of.Visible end) self.dds[#self.dds+1]=df return df end function o:Textbox(t,def,cb)local fr=Instance.new("Frame",self.f) fr.Size=UDim2.new(1,-20,0,40) fr.BackgroundColor3=C3(15,100,50) fr.BorderSizePixel=0 fr.LayoutOrder=#self.btns+#self.dds+#self.sls+1 local lbl=Instance.new("TextLabel",fr) lbl.Size=UDim2.new(0.5,-10,1,0) lbl.Position=UDim2.new(0,10,0,0) lbl.BackgroundTransparency=1 lbl.TextColor3=C3(210,255,210) lbl.Text=t lbl.TextScaled=true lbl.TextXAlignment=Enum.TextXAlignment.Left local tb=Instance.new("TextBox",fr) tb.Size=UDim2.new(0.45,-10,0,25) tb.Position=UDim2.new(0.5,5,0.5,-12) tb.BackgroundColor3=C3(25,130,70) tb.TextColor3=C3(210,255,210) tb.Text=def or "" tb.ClearTextOnFocus=false tb.TextScaled=true tb.BorderSizePixel=0 Instance.new("UICorner",tb) tb.FocusLost:Connect(function(enter)if enter and cb then cb(tb.Text)end end) self.tbs[#self.tbs+1]=tb return tb end return o end return UI
