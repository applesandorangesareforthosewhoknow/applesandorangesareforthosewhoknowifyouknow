local U={}do local u=game:GetService("UserInputService") local c=game:GetService("CoreGui") local s=Instance.new("ScreenGui") s.Name="SUI" s.ResetOnSpawn=false s.Parent=c local function n(t,p)local o=Instance.new(t)if p then o.Parent=p end return o end local a={b={},t={},l={},d={},x={}} function U:B(t,f)local b=n("TextButton",s) b.Size=UDim2.new(0,120,0,30) b.BackgroundColor3=Color3.fromRGB(40,40,40) b.Text=t b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=18 b.MouseButton1Click:Connect(f) a.b[#a.b+1]=b return b end function U:T(t,d,f)local b=n("TextButton",s) b.Size=UDim2.new(0,120,0,30) b.BackgroundColor3=Color3.fromRGB(40,40,40) b.Text=t..": "..(d and"ON"or"OFF") b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=18 local st=d b.MouseButton1Click:Connect(function() st=not st b.Text=t..": "..(st and"ON"or"OFF") f(st) end) a.t[#a.t+1]=b return b end function U:L(t,d,f)local fr=n("Frame",s) fr.Size=UDim2.new(0,150,0,40) fr.BackgroundTransparency=1 local lb=n("TextLabel",fr) lb.Size=UDim2.new(1,0,0,20) lb.BackgroundTransparency=1 lb.Text=t..": "..d lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local bar=n("Frame",fr) bar.Position=UDim2.new(0,0,0,22) bar.Size=UDim2.new(1,0,0,15) bar.BackgroundColor3=Color3.fromRGB(70,70,70) bar.BorderSizePixel=0 local fill=n("Frame",bar) fill.Size=UDim2.new(d/100,0,1,0) fill.BackgroundColor3=Color3.fromRGB(170,170,170) fill.BorderSizePixel=0 local dr=false bar.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then dr=true end end) bar.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then dr=false end end) u.InputChanged:Connect(function(i) if dr and i.UserInputType==Enum.UserInputType.MouseMovement then local x=math.clamp(i.Position.X-bar.AbsolutePosition.X,0,bar.AbsoluteSize.X) local v=math.floor((x/bar.AbsoluteSize.X)*100) fill.Size=UDim2.new(v/100,0,1,0) lb.Text=t..": "..v f(v) end end) a.l[#a.l+1]=fr return fr end function U:D(t,o,f)local fr=n("Frame",s) fr.Size=UDim2.new(0,120,0,30) fr.BackgroundColor3=Color3.fromRGB(40,40,40) fr.BorderSizePixel=0 local lb=n("TextLabel",fr) lb.Size=UDim2.new(1,0,1,0) lb.BackgroundTransparency=1 lb.Text=t..": "..o[1] lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local open=false local of=n("Frame",s) of.Size=UDim2.new(0,120,#o*25,0) of.Position=UDim2.new(0,fr.AbsolutePosition.X,0,fr.AbsolutePosition.Y+30) of.BackgroundColor3=Color3.fromRGB(40,40,40) of.Visible=false of.ClipsDescendants=true for i=1,#o do local b=n("TextButton",of) b.Size=UDim2.new(1,0,0,25) b.Position=UDim2.new(0,0,0,(i-1)*25) b.BackgroundColor3=Color3.fromRGB(50,50,50) b.Text=o[i] b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=16 b.MouseButton1Click:Connect(function() lb.Text=t..": "..o[i] f(o[i]) of.Visible=false open=false end) end fr.MouseButton1Click:Connect(function() open=not open of.Position=UDim2.new(0,fr.AbsolutePosition.X,0,fr.AbsolutePosition.Y+30) of.Visible=open end) a.d[#a.d+1]=fr return fr end function U:X(t,f)local fr=n("Frame",s) fr.Size=UDim2.new(0,150,0,30) fr.BackgroundColor3=Color3.fromRGB(40,40,40) fr.BorderSizePixel=0 local lb=n("TextLabel",fr) lb.Size=UDim2.new(0.4,0,1,0) lb.BackgroundTransparency=1 lb.Text=t lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local tb=n("TextBox",fr) tb.Size=UDim2.new(0.6,0,1,0) tb.Position=UDim2.new(0.4,0,0,0) tb.BackgroundColor3=Color3.fromRGB(70,70,70) tb.TextColor3=Color3.new(1,1,1) tb.ClearTextOnFocus=false tb.Font=Enum.Font.SourceSans tb.TextSize=18 tb.Text="" tb.FocusLost:Connect(function(e) if e then f(tb.Text) end end) a.x[#a.x+1]=fr return fr end function U:C() for _,v in next,a.b do v:Destroy() end for _,v in next,a.t do v:Destroy() end for _,v in next,a.l do v:Destroy() end for _,v in next,a.d do v:Destroy() end for _,v in next,a.x do v:Destroy() end end end return U
