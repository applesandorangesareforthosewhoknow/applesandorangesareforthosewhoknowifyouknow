local l={};local g=Instance;function l:cw(t,s,k)local f=g.new("ScreenGui",game:GetService("CoreGui"));f.ResetOnSpawn=false;local w=g.new("Frame",f)w.Size=s;w.Position=UDim2.new(0.5,-s.X.Offset/2,0.5,-s.Y.Offset/2)w.BackgroundColor3=Color3.fromRGB(30,45,30)w.BorderSizePixel=0;w.Active=true;w.Draggable=true;local tl=g.new("TextLabel",w)tl.Size=UDim2.new(1,0,0,30)tl.Text=t;tl.TextColor3=Color3.new(1,1,1)tl.BackgroundTransparency=1;tl.Font=Enum.Font.GothamBold;tl.TextSize=16;local b=g.new("UIListLayout",w)b.Padding=UDim.new(0,4)b.SortOrder=Enum.SortOrder.LayoutOrder;tl.LayoutOrder=0;function l:NewTab(n)local p={};local f=g.new("Frame",w)f.Size=UDim2.new(1,0,0,300)f.BackgroundTransparency=1;f.LayoutOrder=1;g.new("TextLabel",f).Text=n;function p:Button(t,c)local x=g.new("TextButton",f)x.Size=UDim2.new(1,-10,0,30)x.Text=t;x.BackgroundColor3=Color3.fromRGB(40,60,40)x.TextColor3=Color3.new(1,1,1)x.MouseButton1Click:Connect(c)end;function p:Slider(t,mx,mn,d,c)local h=g.new("Frame",f)h.Size=UDim2.new(1,-10,0,30)h.BackgroundTransparency=1;local s=g.new("TextLabel",h)s.Text=t;s.Size=UDim2.new(0.5,-5,1,0)s.BackgroundTransparency=1;s.TextColor3=Color3.new(1,1,1)s.TextXAlignment=Enum.TextXAlignment.Left;local sl=g.new("TextBox",h)sl.Size=UDim2.new(0.5,-5,1,0)sl.Position=UDim2.new(0.5,5,0,0)sl.Text=tostring(d)sl.TextColor3=Color3.new(1,1,1)sl.BackgroundColor3=Color3.fromRGB(30,50,30)sl.ClearTextOnFocus=false;sl.FocusLost:Connect(function()local v=tonumber(sl.Text)if v then c(v)end end)end;function p:Textbox(t,d,c)local x=g.new("TextBox",f)x.Size=UDim2.new(1,-10,0,30)x.Text=d;x.PlaceholderText=t;x.TextColor3=Color3.new(1,1,1)x.BackgroundColor3=Color3.fromRGB(30,50,30)x.ClearTextOnFocus=true;x.FocusLost:Connect(function()c(x.Text)end)end;function p:Dropdown(t,o,c)local d=g.new("TextButton",f)d.Size=UDim2.new(1,-10,0,30)d.Text=t;d.TextColor3=Color3.new(1,1,1)d.BackgroundColor3=Color3.fromRGB(40,60,40);d.MouseButton1Click:Connect(function()for i=1,#o do local it=g.new("TextButton",f)it.Size=UDim2.new(1,-20,0,25)it.Text=o[i]it.TextColor3=Color3.new(1,1,1)it.BackgroundColor3=Color3.fromRGB(20,40,20)it.MouseButton1Click:Connect(function()c(o[i])it:Destroy()end)end end)end;return p end;return l
