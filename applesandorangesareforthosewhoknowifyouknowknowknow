local U={}do local u=game:GetService("UserInputService") local c=game:GetService("CoreGui") local s=Instance.new("ScreenGui") s.Name="SUI" s.ResetOnSpawn=false s.Parent=c local vis=true local function n(t,p)local o=Instance.new(t)if p then o.Parent=p end return o end local a={b={},t={},l={},d={},x={},tabs={},btns={}} local win=n("Frame",s) win.Size=UDim2.new(0,400,0,300) win.Position=UDim2.new(0.5,-200,0.5,-150) win.BackgroundColor3=Color3.fromRGB(30,30,30) win.Active=true win.Draggable=true local tabbtnfr=n("Frame",win) tabbtnfr.Size=UDim2.new(1,0,0,30) tabbtnfr.BackgroundColor3=Color3.fromRGB(40,40,40) tabbtnfr.BorderSizePixel=0 local contfr=n("Frame",win) contfr.Position=UDim2.new(0,0,0,30) contfr.Size=UDim2.new(1,0,1,-30) contfr.BackgroundColor3=Color3.fromRGB(25,25,25) contfr.BorderSizePixel=0 u.InputBegan:Connect(function(i,gp) if gp then return end if i.KeyCode==Enum.KeyCode.Delete then if vis then s.Parent=nil s.Enabled=false vis=false else s.Parent=c s.Enabled=true vis=true end end end) function U:CreateTab(name)local btn=n("TextButton",tabbtnfr) btn.Size=UDim2.new(0,100,1,0) btn.BackgroundColor3=Color3.fromRGB(50,50,50) btn.BorderSizePixel=0 btn.Text=name btn.TextColor3=Color3.new(1,1,1) btn.Font=Enum.Font.SourceSans btn.TextSize=18 btn.Position=UDim2.new(#a.btns*100,0,0,0) local frame=n("Frame",contfr) frame.Size=UDim2.new(1,0,1,0) frame.BackgroundTransparency=1 frame.Visible=false a.tabs[#a.tabs+1]=frame a.btns[#a.btns+1]=btn btn.MouseButton1Click:Connect(function() for i=1,#a.tabs do a.tabs[i].Visible=false a.btns[i].BackgroundColor3=Color3.fromRGB(50,50,50) end frame.Visible=true btn.BackgroundColor3=Color3.fromRGB(80,80,80) end) if #a.tabs==1 then btn.MouseButton1Click:Invoke() or btn.MouseButton1Click:Wait() end return frame end local function place(obj,p) if p then obj.Parent=p else obj.Parent=contfr end end function U:B(t,f,p)local b=n("TextButton") b.Size=UDim2.new(0,120,0,30) b.BackgroundColor3=Color3.fromRGB(40,40,40) b.Text=t b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=18 b.MouseButton1Click:Connect(f) place(b,p) a.b[#a.b+1]=b return b end function U:T(t,d,f,p)local b=n("TextButton") b.Size=UDim2.new(0,120,0,30) b.BackgroundColor3=Color3.fromRGB(40,40,40) b.Text=t..": "..(d and"ON"or"OFF") b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=18 local st=d b.MouseButton1Click:Connect(function() st=not st b.Text=t..": "..(st and"ON"or"OFF") f(st) end) place(b,p) a.t[#a.t+1]=b return b end function U:L(t,d,f,p)local fr=n("Frame") fr.Size=UDim2.new(0,150,0,40) fr.BackgroundTransparency=1 local lb=n("TextLabel",fr) lb.Size=UDim2.new(1,0,0,20) lb.BackgroundTransparency=1 lb.Text=t..": "..d lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local bar=n("Frame",fr) bar.Position=UDim2.new(0,0,0,22) bar.Size=UDim2.new(1,0,0,15) bar.BackgroundColor3=Color3.fromRGB(70,70,70) bar.BorderSizePixel=0 local fill=n("Frame",bar) fill.Size=UDim2.new(d/100,0,1,0) fill.BackgroundColor3=Color3.fromRGB(170,170,170) fill.BorderSizePixel=0 local dr=false bar.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then dr=true end end) bar.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then dr=false end end) u.InputChanged:Connect(function(i) if dr and i.UserInputType==Enum.UserInputType.MouseMovement then local x=math.clamp(i.Position.X-bar.AbsolutePosition.X,0,bar.AbsoluteSize.X) local v=math.floor((x/bar.AbsoluteSize.X)*100) fill.Size=UDim2.new(v/100,0,1,0) lb.Text=t..": "..v f(v) end end) place(fr,p) a.l[#a.l+1]=fr return fr end function U:D(t,o,f,p)local fr=n("Frame") fr.Size=UDim2.new(0,120,0,30) fr.BackgroundColor3=Color3.fromRGB(40,40,40) fr.BorderSizePixel=0 local lb=n("TextLabel",fr) lb.Size=UDim2.new(1,0,1,0) lb.BackgroundTransparency=1 lb.Text=t..": "..o[1] lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local open=false local of=n("Frame",s) of.Size=UDim2.new(0,120,#o*25,0) of.Position=UDim2.new(0,fr.AbsolutePosition.X,0,fr.AbsolutePosition.Y+30) of.BackgroundColor3=Color3.fromRGB(40,40,40) of.Visible=false of.ClipsDescendants=true for i=1,#o do local b=n("TextButton",of) b.Size=UDim2.new(1,0,0,25) b.Position=UDim2.new(0,0,0,(i-1)*25) b.BackgroundColor3=Color3.fromRGB(50,50,50) b.Text=o[i] b.TextColor3=Color3.new(1,1,1) b.BorderSizePixel=0 b.Font=Enum.Font.SourceSans b.TextSize=16 b.MouseButton1Click:Connect(function() lb.Text=t..": "..o[i] f(o[i]) of.Visible=false open=false end) end fr.MouseButton1Click:Connect(function() open=not open of.Position=UDim2.new(0,fr.AbsolutePosition.X,0,fr.AbsolutePosition.Y+30) of.Visible=open end) place(fr,p) a.d[#a.d+1]=fr return fr end function U:X(t,f,p)local fr=n("Frame") fr.Size=UDim2.new(0,150,0,30) fr.BackgroundColor3=Color3.fromRGB(40,40,40) fr.BorderSizePixel=0 local lb=n("TextLabel",fr) lb.Size=UDim2.new(0.4,0,1,0) lb.BackgroundTransparency=1 lb.Text=t lb.TextColor3=Color3.new(1,1,1) lb.Font=Enum.Font.SourceSans lb.TextSize=18 lb.TextXAlignment=Enum.TextXAlignment.Left local tb=n("TextBox",fr) tb.Size=UDim2.new(0.6,0,1,0) tb.Position=UDim2.new(0.4,0,0,0) tb.BackgroundColor3=Color3.fromRGB(70,70,70) tb.TextColor3=Color3.new(1,1,1) tb.ClearTextOnFocus=false tb.Font=Enum.Font.SourceSans tb.TextSize=18 tb.Text="" tb.FocusLost:Connect(function(e) if e then f(tb.Text) end end) place(fr,p) a.x[#a.x+1]=fr return fr end function U:C() for _,v in next,a.b do v:Destroy() end for _,v in next,a.t do v:Destroy() end for _,v in next,a.l do v:Destroy() end for _,v in next,a.d do v:Destroy() end for _,v in next,a.x do v:Destroy() end end end return U
